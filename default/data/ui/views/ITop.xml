  <row>
    <panel>
      <single>
        <title>#anomalies</title>
        <search>
          <query>sourcetype="paydata" |transaction source startswith="message received" |timechart span=5m count, avg(duration) as duration|anomalydetection method=histogram action=summary|fields num_anomalies</query>
          <earliest>0</earliest>
          <latest></latest>
        </search>
