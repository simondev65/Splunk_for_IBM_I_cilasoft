        <option name="underLabel">Anomalies</option>
        <option name="useColors">1</option>
        <option name="useThousandSeparators">1</option>
      </single>
    </panel>
    <panel>
      <table>
        <title>transaction source count anomaly</title>
        <search>
          <query>sourcetype="paydata" |transaction source startswith="message received" |timechart span=5m count|anomalydetection</query>
          <earliest>0</earliest>
          <latest></latest>
        </search>
      </table>
    </panel>
    <panel>
      <table>
        <title>Anomalies with zscore</title>
        <search>
          <query>sourcetype="paydata" |transaction source startswith="message received" |timechart span=5m count, avg(duration) as duration|anomalydetection method=zscore action=filter pthresh=0.05</query>
          <earliest>0</earliest>
        </search>
        <option name="wrap">true</option>
        <option name="rowNumbers">false</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">cell</option>
        <option name="count">10</option>
      </table>
    </panel>
  </row>
